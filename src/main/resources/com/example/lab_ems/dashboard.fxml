<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.VBox?>

<VBox alignment="CENTER" spacing="20.0" xmlns="http://javafx.com/javafx/23" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.lab_ems.DashboardController">
    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
    </padding>
   <children>
      <Button mnemonicParsing="false" fx:id="adminbtn" onAction="#Admin" text="Admin" />
      <Button mnemonicParsing="false" fx:id="employeebtn" onAction="#Employee" text="Employee" />
      <Button mnemonicParsing="false" fx:id="exitbtn" onAction="#Exit" text="Exit" />
      <Button mnemonicParsing="false" fx:id="logoutbtn" onAction="#Logout" text="Logout" />
   </children>

</VBox>
